    <!-- CSS -->
    <link rel="stylesheet" href="../../public/css/style.css">

    <!-- Slider Extras -->
    <% if (sliderRequired) { %>
        <%- include('slider') %>
    <% } %>

    <!-- Title -->
    <title><%= title %></title>
</head>
<body>
    <% if (navbarRequired) { %>
        <% if (navbar1) { %>
            <%- include ('navbar1') %>
        <% } %>
        <% if (navbar2) { %>
            <%- include ('navbar2') %>
        <% } %>
    <% } %>
    
    <%- include('bookmark') %>

    <%- include ('footer') %>
</body>
</html>